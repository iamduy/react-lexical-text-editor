    "lexical-text-editor"
  ],
  "author": "SanMario",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamduy/react-lexical-text-editor.git"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
