{
  "name": "react-lexical-editor",
  "version": "1.0.0",
  "type": "module",
  "description": "A rich text editor based on Lexical with images, tables, links, etc.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "rollup": "rollup -c --bundleConfigAsCjs",
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist --clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "lexical",
    "rich-text",
    "editor",
    "react"
  ],
  "author": "SanMario",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamduy/react-lexical-editor.git"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss-url": "^10.1.3",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@lexical/html": "^0.39.0",
    "@lexical/link": "^0.39.0",
    "@lexical/list": "^0.39.0",
    "@lexical/react": "^0.39.0",
    "@lexical/rich-text": "^0.39.0",
    "@lexical/selection": "^0.39.0",
    "@lexical/table": "^0.39.0",
    "@lexical/utils": "^0.39.0",
    "@types/uuid": "^10.0.0",
    "lexical": "^0.39.0",
    "uuid": "^13.0.0"
  }
}
